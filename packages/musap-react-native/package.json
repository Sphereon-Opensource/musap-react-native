{
  "name": "@sphereon/musap-react-native",
  "title": "musap-rn",
  "version": "0.0.2",
  "description": "A React Native module wrapping the MUSAP library for Android & iOS",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "nativePackage": true,
  "files": [
    "src",
    "dist",
    "android-release.aar"
  ],
  "scripts": {
    "build": "tsc --build --clean && tsc --build",
    "prepare": "yarn build",
    "build:android:windows": "cd android && gradlew.bat bundleReleaseAar",
    "build:android:linux": "cd android && ./gradlew bundleReleaseAar",
    "build:ios": "cd ios && xcodebuild -scheme MusapRn -sdk iphonesimulator -destination 'platform=iOS Simulator,name=iPhone 12' build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sphereon-Opensource/musap-react-native.git",
    "baseUrl": "https://github.com/Sphereon-Opensource/musap-react-native"
  },
  "keywords": [
    "react-native",
    "musap"
  ],
  "author": {
    "name": "Sphereon",
    "email": "dev@sphereon.com"
  },
  "license": "MIT",
  "licenseFilename": "LICENSE",
  "readmeFilename": "README.md",
  "dependencies": {
    "react": ">=16.8.1",
    "react-native": ">=0.60.0-rc.0 <1.0.x"
  },
  "devDependencies": {
    "@react-native-community/cli": "^13.6.9",
    "@react-native/typescript-config": "^0.74.85",
    "@types/react": "^18.3.3",
    "@types/react-native": "^0.73.0",
    "react": "^18.3.1",
    "react-native": "^0.74.3"
  },
  "gitHead": "81bc614f7a93b9d09c4a0d94fc23a6b9a7d44c0e",
  "publishConfig": {
    "access": "public"
  }
}
