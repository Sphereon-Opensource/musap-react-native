{
  "extends": "../tsconfig-base.json",
  "compilerOptions": {
    "target": "es2016",
    "rootDir": "src",
    "outDir": "./dist",
    "baseUrl": "./",
    "types": ["react-native"],
    "paths": {
      "musap-rn": ["./src/index"]
    },
    "noEmit": false,
    "emitDeclarationOnly": false,
    "allowImportingTsExtensions": false,
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
